<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical">

   <!-- <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
